{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceRoot}/src/WebApi/bin/Debug/netcoreapp2.0/WebApi.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/WebApi",
            "preLaunchTask": "build",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "development",
                "ASPNETCORE_URLS": "http://localhost:5000"
            },
            "stopAtEntry": false,
            "externalConsole": true
        }
    ]
}